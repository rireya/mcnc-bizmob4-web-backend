package com.mcnc.sm.hiplus.domain.commonCode.dto.response;

import java.util.List;

import com.mcnc.bizmob.web.domain.commonCode.dto.CommonCodeDto;

import lombok.AccessLevel;
import lombok.Builder;
import lombok.Getter;
import lombok.NoArgsConstructor;

@Getter
@NoArgsConstructor(access = AccessLevel.PROTECTED)
public class GetBoardCategoryListResponse {
	private List<CommonCodeDto> categoryList;
	private String parentCodeId;
	private String parentCodeName;
	@Builder
	public GetBoardCategoryListResponse(List<CommonCodeDto> categoryList, String parentCodeId, String parentCodeName) {
		this.categoryList = categoryList;
		this.parentCodeId = parentCodeId;
		this.parentCodeName = parentCodeName;
	}
}
